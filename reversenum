import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    int a, rev=0;
	Scanner sc = new Scanner(System.in);
	System.out.println("Enter a number");
	a=sc.nextInt();
	while(a>0)
	{ 
	    rev=rev+a%10;
	    a=a/10;
	    rev=rev*10;
	   
	}
	rev=rev/10;
	System.out.println(rev);
	}
	
	
}
