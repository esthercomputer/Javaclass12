import java.util.*;
public class Main
{
    public static void main(String args[])
    {
        int arr[]={3,3,4,7,1};
        for(int i=0;i<5;i++)
        {
            for(int j=i+1;j<5;j++)
            {
                if (arr[i]==arr[j])
                arr[j]=0;
            }
            
        }
        System.out.println("The new array:" );
        for(int i=0;i<5;i++)
        System.out.println(arr[i]);
    }
}
